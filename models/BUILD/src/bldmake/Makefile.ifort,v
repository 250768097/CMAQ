@# make file to build bldmake program

PROGRAM = bldmake

FC = ifort
FC_FLAGS = -c -O2 -fixed -extend_source -WB 

OBJS= cfg_module.o parser.o utils.o bldmake.o

.SUFFIXES: .F .f .c

$(PROGRAM):	$(OBJS)
	$(FC) $(OBJS) -o $(PROGRAM)

.F.o:
	$(FC) $(FC_FLAGS) $<

clean:
	rm *.o *.mod bldmake

@


