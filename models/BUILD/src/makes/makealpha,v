@# make file to compile for ALPHA

CC = cc
CFLAGS = -std1 -g
CLITEFLAGS = -g

OBJECTS =	fw_util.o \
		m3bld.o \
		bld_parser.o \
		bld_scanner.o \
		action.o \
		parser.o \
		sms.o 

