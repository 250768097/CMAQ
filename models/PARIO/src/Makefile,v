 pwrgrdd.o \
 readbndy.o \
 subdmap.o \
 wrsubmap.o \
 parutilio.o

.SUFFIXES: .f

$(LIBNAME) : $(OBJS)
	$(FC) $(FFLAGS) -c $(?:.o=.f)
	ar -rv $@@ $?
	chmod 644 $@@

clean:
	rm -f $(OBJS) $(LIBNAME) *.mod

