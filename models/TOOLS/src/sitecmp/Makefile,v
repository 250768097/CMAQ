@# make file to build sitecmp program

# RCS file, release, date & time of last delta, author, state, [and locker]
# $Header: /project/work/rep/TOOLS/src/sitecmp/Makefile,v 1.2 2008/09/23 16:47:58 sjr Exp $ 

PROGRAM = sitecmpV1.0.exe

# Include file subsitution definitions
INCLUDES = -DSUBST_GRID_ID= \
 -DSUBST_IOPARMS=\'/share/linux9.0/ioapi30_pg/include/PARMS3.EXT\' \
 -DSUBST_IOFDESC=\'/share/linux9.0/ioapi30_pg/include/FDESC3.EXT\' \
 -DSUBST_IODECL=\'/share/linux9.0/ioapi30_pg/include/IODECL3.EXT\' \
 -DSUBST_XSTAT=\'/share/linux9.0/ioapi30_pg/include/XSTAT3.EXT\' 

FC = pgf90
FC_FLAGS = -c -Bstatic -Mextend -Mbounds -Mfixed $(INCLUDES)

LIBRARIES = -L /share/linux9.0/ioapi30_pg/lib -lioapi \
            -L /share/linux9.0/netcdf_3.5/lib -lnetcdf 


OBJS=	\
 module_envvar.o \
 module_file.o \
 module_grid.o \
 module_sites.o \
 module_tstep.o \
 module_spec.o \
 ck_ctms.o \
 get_gridval.o \
 parser.o \
 get_units.o \
 process.o \
 sitecmp.o


$(PROGRAM):  $(OBJS)
	$(FC) $(OBJS) $(LIBRARIES) -o $(PROGRAM)

.F.o:
	$(FC) $(FC_FLAGS) $<


@


